{
	"name": "HSK Overlay",
	"version": "0.0.1",
	"description": "Highlights HSK vocabulary on Chinese language websites",
	"icons": {},
	"content_scripts": [{
		"all_frames": true,
		"css": ["css/content.css"],
		"js": [
			"js/lib/jquery-3.2.1.min.js",
			"js/main.js"
		],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}],
	"permissions": [
		"contentSettings",
		"<all_urls>"
	],
	"manifest_version": 2
}
